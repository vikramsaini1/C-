#include <iostream>
using namespace std;

int main()
{
	int rows;
	cout<<"Enter the row"<<endl;
	cin>>rows;

	char character;
	cout<<"Enter the character"<<endl;
	cin>>character;
	
	for (int i =1; i <= rows; ++i) {
	
	
		for (int j = 1; j <= i; j++) {
		
		
			cout <<character;
		}
	cout<<endl;
	character ++;
	}
	return 0;
}
