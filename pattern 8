#include <iostream>
using namespace std;

int main()
{
	int rows;
	cout<<"Enter the row"<<endl;
	cin>>rows;

	int number;
	cout<<"Enter the number"<<endl;
	cin>>number;
	
	for (int i =1; i < rows; ++i) {
	
	
		for (int j = 1; j <= i; j++) {
		
		
			cout <<number<<" ";
			number ++;
		}
	cout<<endl;
	
	}
	return 0;
}
